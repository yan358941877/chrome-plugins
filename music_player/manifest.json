{
    "manifest_version": 2,
    "name": "音乐FM",
    "version": "2.0",
    "description": "my first MusicPlayer plugin",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "音乐FM",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    }
}